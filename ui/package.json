{
  "name": "@keymap.click/ui",
  "version": "2.0.1",
  "private": false,
  "type": "module",
  "main": "dist/keymap.click.js",
  "types": "dist/keymap.click.d.ts",
  "scripts": {
    "build": "esbuild --bundle --sourcemap --format=esm --loader:.css=text --minify --outfile=dist/keymap.click.js src/index.ts",
    "postbuild": "tsc --project tsconfig.json && dts-bundle-generator --config dts-bundle-generator.json",
    "keymap.click.watch": "esbuild --bundle --sourcemap --format=esm --loader:.css=text --minify --outfile=../www/static/keymap.click/keymap.click.js src/index.ts --watch=forever"
  },
  "devDependencies": {
    "@types/node": "^24.0.3",
    "dts-bundle-generator": "^9.5.1",
    "esbuild": "^0.25.5",
    "eslint": "^9.29.0",
    "prettier": "3.5.3",
    "typescript": "^5.8.3"
  }
}
