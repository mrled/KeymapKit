{
  "name": "@keymapkit/cli",
  "description": "CLI tools for KeymapKit",
  "author": "Micah R Ledbetter",
  "license": "MIT",
  "version": "1.0.5",
  "private": false,
  "type": "commonjs",
  "bin": {
    "keymapkit-cli": "./dist/cli.js"
  },
  "main": "./dist/cli.js",
  "files": [
    "dist/cli.js"
  ],
  "scripts": {
    "build": "esbuild --bundle --sourcemap --format=cjs --platform=node --outfile=dist/cli.js --banner:js='#!/usr/bin/env node' --external:@keymapkit/ui --external:jsdom src/cli.ts && chmod +x dist/cli.js",
    "test": "node --test src/test/*.test.js",
    "prepack": "npm run build"
  },
  "dependencies": {
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@keymapkit/models": "*",
    "@types/node": "^22.0.0"
  }
}
